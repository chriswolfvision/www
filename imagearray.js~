ARR[19] = new Image(); ARR[19].src = "miscphotos/chris17.jpg";
curIndex=0;

function nextImageInArray() 
{
	curIndex=curIndex+1;
	if (curIndex>19)
		curIndex=0;
	node = document.getElementById("imagearraytarget");
	node.src=ARR[curIndex].src;
}
		
